{
  "name": "quasar-project",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "Daniil Ventsov <daniil.ventsov@progforce.com>",
  "private": true,
  "scripts": {
    "serve": "npx quasar dev",
    "build": "npx quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "build:pwa": "npx quasar build -m pwa",
    "copy-key": "cp ./keys/my-release-key.keystore.old ./dist/capacitor/android/apk/release/",
    "sign:apk": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore ./dist/capacitor/android/apk/release/my-release-key.keystore.old ./dist/capacitor/android/apk/release/app-release-unsigned.apk alias_name",
    "zip:apk": "zipalign -v 4 ./dist/capacitor/android/apk/release/app-release-unsigned.apk ./dist/capacitor/android/apk/release/quasar.apk",
    "build:apk": "npx quasar build -m capacitor -T android && npm run copy-key && npm run sign:apk && npm run zip:apk"
  },
  "dependencies": {
    "@capacitor/core": "^4.0.1",
    "@capacitor/push-notifications": "^4.1.1",
    "@quasar/extras": "^1.0.0",
    "@socket.io/mongo-adapter": "^0.2.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^4.8.1",
    "mongoose": "^6.4.6",
    "peerjs": "^1.4.7",
    "quasar": "^2.7.5",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "ts-node-dev": "^2.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.0.0",
    "vue-3-socket.io": "^1.0.5",
    "vue-i18n": "^9.0.0",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.0.0",
    "vue-socket.io": "^3.0.10",
    "vue3-qrcode-reader": "^0.0.1",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@quasar/app-webpack": "^3.0.0",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.20.21",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "electron": "^21.2.0",
    "electron-packager": "^15.5.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
